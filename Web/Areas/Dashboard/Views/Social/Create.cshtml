@model Social


<div class="card">
    <div class="card-body">
        <form method="post">
            <div class="form-group">
                <input asp-for="Icon" class="form-control" placeholder="Icon" />
            </div>
            <div class="form-group">
                <input asp-for="BaseUrl" class="form-control" placeholder="Base url" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>