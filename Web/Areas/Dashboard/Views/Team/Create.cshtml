@model Team

<div class="card">
    <div class="card-body">
        <form method="post">
            <div class="form-group">
                <input asp-for="Name" class="form-control" placeholder="Name" />
            </div>
            <div class="form-group">
                <input asp-for="Surname" class="form-control" placeholder="Surname" />
            </div>
            <div class="form-group">
                <input asp-for="PhotoUrl" class="form-control" placeholder="PhotoUrl" />
            </div>



            <div class="form-group">
                @foreach (var item in ViewData["social"] as List<Social>)
                {
                    <div class="d-flex">
                       <input name="SocialId" type="hidden" value="@item.Id" />
                       <span>@item.BaseUrl</span> <input name="SocialUrl" class="form-control" type="text" />
                    </div>
                    
                }
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

